@model List<SharedObjects.ValueObjects.VSummary>

<table id="tbl-debugresult" class="table table-bordered table-hover table-striped">
    <thead>
        <tr class="text-center bold">
            @*<td>Fiscal Year</td>*@
            <td>Month</td>
            <td>Forecasted Volume</td>
            <td>Calculated Debug Operator</td>
            <td>Calculated Debug Technician</td>
            @*<td>Calculated Test Operator</td>*@
            <td>Calculated Test Technician</td>
            <td>Locked Debug Operator</td>
            <td>Locked Debug Technician</td>
            <td>Locked Test Technician</td>
        </tr>
    </thead>
    <tbody id="table-data">
        @foreach (var item in Model)
        {
        <tr class="text-center">
            @*<td>@item.FiscalYear</td>*@
            <td>@item.Month</td>
            <td>@item.ForecastedVolume</td>
            <td>@item.CalculatedOP</td>
            <td>@item.CalculatedTech</td>
            @*<td>@item.TestOP</td>*@
            <td>@item.TestTech</td>
            <td>@item.LockedDebugOperator</td>
            <td>@item.LockedDebugTech</td>
            <td>@item.LockedTestHeadCount</td>

        </tr>
        }
    </tbody>
</table>